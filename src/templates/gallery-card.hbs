{{#each this}}
<li class="film__card">        
        <img class="film__img" src="https://image.tmdb.org/t/p/w500{{this.poster_path}}" alt=""/>
        <h2 class="film__name">{{this.original_title}}</h2>
            <div class="film__atribute">
                 <p class="film__ganre">{{this.genre_ids}}</p>
                <p class="film__year">{{this.release_date}}</p>
            </div>
    </li>
    {{/each}}